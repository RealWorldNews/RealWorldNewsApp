
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model Article {
  id        String     @default(cuid()) @id
  slug      String@unique
  headline  String
  summary   String
  body      String
  location  String
  media     String
  date      DateTime
}

